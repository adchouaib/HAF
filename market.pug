mixin cardProjet(projets)
    -const cardStyle = {"width": "12rem","border-color":"black"}
    -const colStyle = {"display":"flex","justify-content":"center","align-items":"center"}
    -const titleStyle = {"font-size":"12px"}
    -const textStyle = {"font-size":"10px","display":"flex","justify-content":"center","align-items":"center"}
    each n in projets 
        .col(style=colStyle)
            .card(style=cardStyle)
                .card-body 
                    -const myattributprojet={"src":"./assets/project.png", "alt":"projectimg" , "class":"img-fluid"}
                        img&attributes(myattributprojet)
                    .card-text(style=textStyle)
                         p #{n.name}      
                    .card-title
                        p(style=titleStyle) #{n.category}
                         p(style=titleStyle) #{n.Price}
                        
                    button(class="btn btn-primary" type="button",style={"font-size": "10px"}) Donate

